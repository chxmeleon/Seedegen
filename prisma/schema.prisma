generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model nftProfitLeaderboard30days {
  id                 String @id @default(auto()) @map("_id") @db.ObjectId
  address            String
  ens                String
  profit             Float
  received           Float
  roi                Float
  spent              Float
  totalTradeTxCounts Int
  totalVolumes       Float
}
