generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model nftProfitLeaderboard30days {
  id                 String   @id @default(auto()) @map("_id") @db.ObjectId
  rank               String
  address            String
  ens                String
  profit             Float
  received           Float
  roi                Float
  spent              Float
  totalTradeTxCounts Int
  totalVolumes       Float
  createAt           DateTime
  updateAt           DateTime
}

model nftProfitLeaderboardAll {
  id                 String   @id @default(auto()) @map("_id") @db.ObjectId
  rank               String
  address            String
  ens                String
  profit             Float
  received           Float
  roi                Float
  spent              Float
  totalTradeTxCounts Int
  totalVolumes       Float
  createAt           DateTime
  updateAt           DateTime
}

model winAndLose {
  id                  String   @id @default(auto()) @map("_id") @db.ObjectId
  rank                String
  walletAddress       String
  walletOpenseaUrl    String
  walletEtherscanUrl  String
  projectEtherscanUrl String
  projectAddress      String
  tokenId             String
  buyTime             String
  buyTxHash           String
  buyTxHashUrl        String
  sellTime            String
  sellTxHash          String
  sellTxHashUrl       String
  status              String
  cost                Float
  got                 Float
  net                 Float
  createdAt           DateTime
  updatedAt           DateTime
}
